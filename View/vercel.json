{
    "rewrites": [
        {
            "source": "/api/:path(.*)",
            "destination": "https://tic-tac-toe-rpl1.onrender.com/api/:path"
        },
        {
            "source": "/socket.io/",
            "destination": "https://tic-tac-toe-rpl1.onrender.com"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}